{
  "auth": {
    "serverlessaairlineauth": {
      "dependsOn": [],
      "frontendAuthConfig": {
        "mfaConfiguration": "OPTIONAL",
        "mfaTypes": [
          "SMS",
          "TOTP"
        ],
        "passwordProtectionSettings": {
          "passwordPolicyCharacters": [
            "REQUIRES_LOWERCASE",
            "REQUIRES_UPPERCASE",
            "REQUIRES_NUMBERS",
            "REQUIRES_SYMBOLS"
          ],
          "passwordPolicyMinLength": 8
        },
        "signupAttributes": [
          "FAMILY_NAME",
          "GIVEN_NAME",
          "PHONE_NUMBER",
          "EMAIL"
        ],
        "socialProviders": [],
        "usernameAttributes": [],
        "verificationMechanisms": [
          "PHONE_NUMBER"
        ]
      },
      "providerPlugin": "awscloudformation",
      "service": "Cognito"
    }
  }
}